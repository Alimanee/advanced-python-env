   "source": [
    "print(\"Столбчатая диаграмма\")\n",
    "\n",
    "students = [\"Али\", \"Мария\", \"Иван\", \"Елена\", \"Дмитрий\"]\n",
    "scores = [85, 92, 78, 95, 88]\n",
    "\n",
    "print(\"Диаграмма:\")\n",
    "for name, score in zip(students, scores):\n",
    "    bars = \"█\" * (score // 10)\n",
    "    print(f\"{name}: {score} {bars}\")"
   ]
  }
